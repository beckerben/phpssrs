    <target name="rs2010">
        <exec command="vendor/bin/wsdl2php -i wsdl/ReportService2010.wsdl -o library/PhpSsrs/ReportingService2010 -n 'PhpSsrs\ReportingService2010' --noIncludes" passthru="true"/>
    </target>
     <target name="res2005">
